<section class="tracks-info">
    <nav class="navbar d-flex justify-content-between">
        <span class="navbar-brand mb-0 h1 font-weight-bold pl-5">Title</span>
        <span class="navbar-brand h1 font-weight-bold pr-5">Listen</span>
    </nav>
    {{#each tracks}}
    <div class="card pb-2">
        <div class="tracks-play d-flex justify-content-between align-items-center">
            <h5 class="track-name pl-2 font-weight-bold">{{this.name}}</h5>
            <audio controls>
                <source src="{{this.preview_url}}" />
            </audio>
        </div>
    </div>
    {{/each}}
</section>